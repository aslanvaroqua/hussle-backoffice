      "plugin": "upload"
    },
    "gtin": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "priceCurrency": {
      "type": "string"
    },
    "itemOffered": {
      "type": "string"
